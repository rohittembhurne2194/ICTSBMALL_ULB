    <script>
        $(document).ready(function () {

            var AppName = $('#myAppName').val();
            if (AppName == "Appynitty") {
                //$("#complaint_menu").css("display", "block");
                $("#AH").css("display", "block");
                $('#AH').show();
            }
            else {
                $("#AH").css("display", "none");
                $('#AH').hide();
            }

        });
    </script>
    <script type="text/javascript">

        function EditSurvey(houseId) {

            
            $('#myModal_House_Survey').modal('toggle');

            $.ajax({
                type: "GET",
                url: "/HouseMaster/GetSurveyFormDetailsById?houseId=" + houseId,
                data: {},
                datatype: "json",
                traditional: true,
                success: function (data) {
                    console.log(data);
                    if (data != null) {
                        $("#divSurveyNoContent").hide();
                        $("#divSurveyContent").show();
                        //$("#txtsvId").val(data.svId);
                        //$("#txthouseId").val(data.houseId);
                        $("#txtReferanceId").val(data.ReferanceId);
                        $("#txthouseLat").val(data.houseLat);
                        $("#txthouseLong").val(data.houseLong);
                        $("#txtname").val(data.name);
                        $("#txtmobileNumber").val(data.mobileNumber);
                        $("#txtage").val(data.age);
                        $("#txtdateOfBirth").val(data.strDateOfBirth);

                        $("#txtgender").val(data.gender);
                        $("#txtbloodGroup").val(data.bloodGroup);
                        $("#txtqualification").val(data.qualification);
                        $("#txtoccupation").val(data.occupation);
                        $("#txtmaritalStatus").val(data.maritalStatus);

                        $("#txtmarriageDate").val(data.strMarriageDate);
                        $("#txtlivingStatus").val(data.livingStatus);
                        $("#txttotalMember").val(data.totalMember);
                        $("#txtwillingStart").val(data.strWillingStart);
                        $("#txtmemberJobOtherCity").val(data.strMemberJobOtherCity);
                        $("#txtnoOfVehicle").val(data.noOfVehicle);
                        //$("#txtvehicleType").val(data.vehicleType);
                        $("#txttwoWheelerQty").val(data.twoWheelerQty);
                        //$("#txtthreeWheelerQty").val(data.threeWheelerQty);
                        $("#txtfourWheelerQty").val(data.fourWheelerQty);
                        $("#txtnoPeopleVote").val(data.noPeopleVote);
                        $("#txtsocialMedia").val(data.socialMedia);
                        $("#txtonlineShopping").val(data.onlineShopping);
                        $("#txtpaymentModePrefer").val(data.paymentModePrefer);
                        $("#txtonlinePayApp").val(data.onlinePayApp);
                        $("#txtinsurance").val(data.insurance);
                        $("#txtunderInsurer").val(data.strUnderInsurer);
                        $("#txtayushmanBeneficiary").val(data.strAyushmanBeneficiary);
                        $("#txtboosterShot").val(data.strBoosterShot);
                        $("#txtmemberDivyang").val(data.strMemberDivyang);
                        $("#txtcreateUserId").val(data.createUserName);
                        $("#txtcreateDate").val(data.strCreateDate);
                        $("#txtupdateUserId").val(data.updateUserName);
                        $("#txtupdateDate").val(data.strUpdateDate);
                        $("#txtresourcesAvailable").val(data.resourcesAvailable);
                        $("#txttotalAdults").val(data.totalAdults);
                        $("#txttotalChildren").val(data.totalChildren);
                        $("#txttotalSrCitizen").val(data.totalSrCitizen);


                    }
                    else {

                        $("#divSurveyNoContent").show();
                        $("#divSurveyContent").hide();
                    }

                },
                error: function (err) {
                    $("#divSurveyNoContent").show();
                    $("#divSurveyContent").hide();
                }
            });
        }








    </script>
